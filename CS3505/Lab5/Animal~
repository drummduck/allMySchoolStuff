//Lab 4

#include <iostream>
#include <string>

using namespace std;

class Animal {
protected:
   double age;
   string name;
public:
   Animal(string n) : name(n), age(0) {};
   virtual void speak() {
      cout << "I am an Animal named " << name << endl;
   }
   virtual void incrementAge() {
   age += 1.0;
   }
}; 